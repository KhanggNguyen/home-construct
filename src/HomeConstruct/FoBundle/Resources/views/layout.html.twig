{% block javascripts %}
    {% javascripts filter='jsqueeze'
        'bundles/symracinetemplate/vendor/jquery/jquery.min.js'
        'bundles/symracinetemplate/vendor/jquery-migrate/jquery-migrate.min.js'
        'bundles/symracinetemplate/vendor/popper.min.js'
