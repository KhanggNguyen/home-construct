        'bundles/symracinetemplate/vendor/noty/lib/noty.min.js'
        'bundles/symracinetemplate/vendor/cookiejs/jquery.cookie.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

    <!-- fin du Jquery -->
    {% if app.flashes('mailSend') is not empty %}
    <script>
        $(document).on('ready', function () {
            var testNoty = new Noty({
                "type": "info",
                "layout": "topRight",
                "timeout": "7000",
                "animation": {
                    "open": "animated fadeIn",
                    "close": "animated fadeOut"
                },
                "closeWith": [
                    "click",
                    "button"
                ],
                "text": "<div class=\"g-mr-20\">" +
                "<div class=\"noty_body__icon\">" +
                "<i class=\"hs-admin-check\">" +
                "</i>" +
                "</div>" +
                "</div>" +
                "<div>" +
                {% for message in app.flashes('notifMail') %}
                "{{ message }}"+
                {% endfor %}
                "</div>",
                "theme": "unify--v1"
            }).show();
        });
    </script>

    {% endif %}


